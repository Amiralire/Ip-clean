#!/bin/bash

# Colors
green='\033[0;32m'
cyan='\033[0;36m'
yellow='\033[1;33m'
red='\033[1;31m'
nc='\033[0m' # No Color

clear
echo -e "${cyan}=============================================="
echo -e "${green}       Welcome to palne ViP Version 1.0"
echo -e "${cyan}==============================================${nc}"
sleep 1

# Country menu
echo -e "${yellow}Select a country to display IP ranges:${nc}"
echo -e "${green}1.${nc} United Arab Emirates"
echo -e "${green}2.${nc} Kuwait"
echo -e "${green}3.${nc} Qatar"
echo -e "${green}4.${nc} Germany"
echo -e "${green}5.${nc} Oman"
echo -ne "${cyan}Enter the country number: ${nc}"
read choice

clear

case $choice in
  1)
    echo -e "${yellow}IP Ranges for United Arab Emirates:${nc}"
    echo "82.178.33.0/24"
    echo "151.248.106.0/24"
    echo "185.95.142.0/24"
    echo "74.221.83.0/24"
    echo "59.160.179.0/24"
    echo "195.229.0.0/17"
    echo "195.229.2.0/24"
    echo "78.159.160.0/24"
    echo "78.159.164.0/24"
    echo "62.215.221.0/24"
    ;;
  2)
    echo -e "${yellow}IP Ranges for Kuwait:${nc}"
    echo "78.100.2.0/24"
    echo "78.100.48.0/24"
    echo "78.101.32.0/24"
    echo "82.148.96.0/24"
    echo "82.148.124.0/24"
    echo "89.211.12.0/24"
    echo "212.77.207.0/24"
    echo "212.77.211.0/24"
    ;;
  3)
    echo -e "${yellow}IP Ranges for Qatar:${nc}"
    echo "84.207.225.0/24"
    echo "84.207.205.0/24"
    echo "84.207.228.0/24"
    echo "84.207.229.0/24"
    echo "89.19.232.0/24"
    ;;
  4)
    echo -e "${yellow}IP Ranges for Germany:${nc}"
    echo "89.19.232.0/24"
    ;;
  5)
    echo -e "${yellow}IP Ranges for Oman:${nc}"
    echo "5.36.0.0/17"
    echo "82.178.159.0/24"
    ;;
  *)
    echo -e "${red}Invalid selection! Please choose a valid number.${nc}"
    ;;
esac

echo ""
echo -e "${cyan}Thank you for using ViP Panel!${nc}"
